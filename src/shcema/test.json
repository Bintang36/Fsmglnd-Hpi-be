{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Unique identifier for the prediction result."
      },
      "hasil gambar": {
        "type": "string",
        "description": "Keyword representing the result of the image classification."
      },
      "penjelasan": {
        "type": "string",
        "description": "Explanation of the prediction."
      },
      "cerita": {
        "type": "string",
        "description": "Story related to the prediction."
      },
      "confidenceScore": {
        "type": "number",
        "description": "Confidence score of the prediction.",
        "minimum": 0,
        "maximum": 100
      },
      "createdAt": {
        "type": "string",
        "format": "date-time",
        "description": "Timestamp when the prediction was created."
      }
    },
    "required": ["id", "hasil gambar", "penjelasan", "cerita", "confidenceScore", "createdAt"],
    "additionalProperties": false
  }
  